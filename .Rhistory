for (i in seq(1:nrow(bribe_data))) {
for (j in seq(1:ncol(bribe_data))) {
z = (bribe_data[i,j] - fe_all[i, j]) /
(sqrt
(fe_all[i,j] * (1- (r[i] / sum(bribe_data)))
* ( 1- (c[j] / sum(bribe_data)))))
z_all <- c(z_all, z)
}}
